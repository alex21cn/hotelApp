{
  "name": "mscms",
  "version": "0.0.0",
  "description": "mscms",
  "author": {
    "name": "xiwang"
  },
  "license": "(ISC OR GPL-3.0)",
  "scripts": {
    "_comments": "specify a different browser for lite-server(browser-sync) in command line doesn't work, don't know the reason, use the bs-config.json configuration instead",
    "start_": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "start": "tsc && concurrently \"tsc -w\" \"lite-server start --browser firefox\" ",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "compile": "tsc && concurrently \"tsc -w\"",
    "dev": "npm run lite"
  },
  "dependencies": {
    "@angular/common": "^2.0.2",
    "@angular/compiler": "^2.0.2",
    "@angular/core": "^2.0.2",
    "@angular/forms": "^2.0.2",
    "@angular/http": "^2.0.2",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "^2.0.2",
    "@angular/platform-browser-dynamic": "^2.0.2",
    "@angular/router": "^3.0.2",
    "@angular/upgrade": "^2.0.2",
    "angular-in-memory-web-api": "^0.1.5",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "lodash": "^4.16.4",
    "ngc": "^1.0.0",
    "node-sass": "^3.10.1",
    "plugin": "^0.3.3",
    "plugin-typescript": "^5.2.7",
    "reflect-metadata": "^0.1.8",
    "systemjs": "^0.19.40",
    "typescript": "^2.0.6",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "angular-2-local-storage": "0.0.19",
    "concurrently": "^2.2.0",
    "jasmine-core": "~2.4.1",
    "jquery": "^3.1.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.2.2",
    "lodash": "^4.11.1",
    "material2": "^2.0.0-alpha.6-2",
    "ng2-completer": "^0.2.2",
    "protractor": "^3.3.0",
    "rxjs": "^5.0.0-rc.1",
    "typescript": "^2.0.2",
    "typings": "^1.4.0"
  }
}
